  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "electron": "^9.0.0",
    "electron-builder": "^22.7.0",
    "electron-is-dev": "^1.2.0",
    "typescript": "^3.9.3",
    "wait-on": "^5.0.0"
  }
